---
apiVersion: networking.k8s.io/v1
kind: Ingress
metadata:
  name: apache-nifi-ingress
  namespace: lec
spec:
  ingressClassName: nginx
  rules:
    - host: nifi-registry.local
      http:
        paths:
          - path: /
            pathType: Prefix
            backend:
              service:
                name: apache-nifi-registry
                port: 
                  number: 18080
